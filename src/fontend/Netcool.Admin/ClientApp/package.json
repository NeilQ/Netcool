{
  "name": "netcool-admin",
  "version": "0.0.0",
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "ng": "ng",
    "start": "ng s",
    "build": "npm run ng-high-memory build",
    "test": "ng test",
    "e2e": "ng e2e",
    "analyze": "npm run ng-high-memory build -- --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "npx stylelint 'src/**/*.less'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.1",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "20.3.1",
    "@angular/compiler": "20.3.1",
    "@angular/core": "20.3.1",
    "@angular/forms": "20.3.1",
    "@angular/platform-browser": "20.3.1",
    "@angular/platform-browser-dynamic": "20.3.1",
    "@angular/platform-server": "20.3.1",
    "@angular/router": "20.3.1",
    "@delon/abc": "^19.2.0",
    "@delon/acl": "^19.2.0",
    "@delon/auth": "^19.2.0",
    "@delon/cache": "^19.2.0",
    "@delon/chart": "^19.2.0",
    "@delon/form": "^19.2.0",
    "@delon/mock": "^19.2.0",
    "@delon/theme": "^19.2.0",
    "@delon/util": "^19.2.0",
    "bootstrap": "^5.3.2",
    "date-fns": "^2.12.0",
    "ng-zorro-antd": "^19.2.1",
    "rxjs": "~7.8.0",
    "screenfull": "^5.0.2",
    "tslib": "^2.0.0",
    "wangeditor": "^4.7.4",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.2",
    "@angular-devkit/core": "^20.3.2",
    "@angular-eslint/builder": "^19.8.1",
    "@angular-eslint/eslint-plugin": "^19.8.1",
    "@angular-eslint/eslint-plugin-template": "^19.8.1",
    "@angular-eslint/schematics": "^19.8.1",
    "@angular-eslint/template-parser": "^19.8.1",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "20.3.1",
    "@angular/language-service": "^20.3.1",
    "@delon/testing": "^19.2.0",
    "@ng-util/monaco-editor": "^19.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@typescript-eslint/utils": "^8.29.1",
    "angular-eslint": "^19.3.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "~2.31.0",
    "eslint-plugin-jsdoc": "~50.6.9",
    "eslint-plugin-prefer-arrow": "~1.2.3",
