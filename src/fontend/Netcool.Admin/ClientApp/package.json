{
  "name": "netcool-admin",
  "version": "0.0.0",
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "ng": "ng",
    "start": "ng s",
    "build": "npm run ng-high-memory build",
    "test": "ng test",
    "e2e": "ng e2e",
    "analyze": "npm run ng-high-memory build -- --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "npx stylelint 'src/**/*.less'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.1",
    "@angular/common": "18.2.1",
    "@angular/compiler": "18.2.1",
    "@angular/core": "18.2.1",
    "@angular/forms": "18.2.1",
    "@angular/platform-browser": "18.2.1",
    "@angular/platform-browser-dynamic": "18.2.1",
    "@angular/platform-server": "18.2.1",
    "@angular/router": "18.2.1",
    "@delon/abc": "^18.1.0",
    "@delon/acl": "^18.1.0",
    "@delon/auth": "^18.1.0",
    "@delon/cache": "^18.1.0",
    "@delon/chart": "^18.1.0",
    "@delon/form": "^18.1.0",
    "@delon/mock": "^18.1.0",
    "@delon/theme": "^18.1.0",
    "@delon/util": "^18.1.0",
    "bootstrap": "^5.3.2",
    "date-fns": "^2.12.0",
    "ng-zorro-antd": "^18.1.0",
    "rxjs": "~7.8.0",
    "screenfull": "^5.0.2",
    "tslib": "^2.0.0",
    "wangeditor": "^4.7.4",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.1",
    "@angular-eslint/builder": "^18.2.0",
    "@angular-eslint/eslint-plugin": "^18.2.0",
    "@angular-eslint/eslint-plugin-template": "^18.2.0",
    "@angular-eslint/schematics": "^18.2.0",
    "@angular-eslint/template-parser": "^18.2.0",
    "@angular/cli": "^18.2.1",
    "@angular/compiler-cli": "18.2.1",
    "@angular/language-service": "^18.1.0",
    "@delon/testing": "^18.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "~2.25.3",
    "eslint-plugin-jsdoc": "~46.8.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-prettier": "~5.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-alain": "^18.1.0",
