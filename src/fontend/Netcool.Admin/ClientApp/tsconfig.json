{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "module": "es2020",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2020",
      "dom"
    ],
    "paths": {
      "@shared": [
        "src/app/shared/index"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@core": [
        "src/app/core/index"
      ],
      "@core/*": [
        "src/app/core/*"
      ],
      "@env/*": [
        "src/environments/*"
      ],
      "@services": [
        "src/app/services/index"
      ],
      "@models": [
        "src/app/models/index"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "useDefineForClassFields": false
  }
}
